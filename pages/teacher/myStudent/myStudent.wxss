/* pages/teacher/myStudent/myStudent.wxss */
.header {
  display: flex;
  padding: 0 40rpx;
  box-sizing: border-box;
  justify-content: space-between;
  font-size: 28rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #f0f0f0;
}

/* 学生列表开始 */
.studentList {
  padding: 10rpx 28rpx;
  box-sizing: border-box;
}
.s_item {
  padding: 22rpx 0 30rpx 0;
  box-sizing: border-box;
  border-bottom: 1rpx solid #d4d4d4;
}
.s_item:last-child {
  border-bottom: none;
}
/* 学生列表结束 */